{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Elasticsearch", "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "Microsoft": "Error",
        "System": "Error"
      }
    },
    "WriteTo": [
      {
        "Name": "Elasticsearch",
        "Args": {
          "nodeUris": "http://antares-vm-agents:9205/",
          "indexFormat": "camila-index",
          "batchPostingLimit": 50,
          "period": 2,
          "connectionTimeout": 5,
          "typeName": "log"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "C:\\REPOSITORIO - CAMILA\\SyncLogsElasticsearch\\Logs\\logs.txt",
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss} | {Level} | Trace:{TraceId} | RequestPath:{RequestPath} | {SourceContext} | {Message} | {Exception}{NewLine}"
        }
      }
    ],
    "Filter": [
      {
        "Name": "ByExcluding",
        "Args": {
          "expression": false
        }
      }
    ]
  },
  "AllowedHosts": "*"
}
